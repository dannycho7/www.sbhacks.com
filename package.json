{
  "name": "sbhacksiv",
  "version": "1.0.0",
  "description": "The SB Hacks IV Website, which enables students to apply for our 36 hour hackathon hosted in Winter 2018",
  "main": "app/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/bin/server.js",
    "start:dev": "./node_modules/.bin/nodemon src/bin/server.js",
    "build": "npm run css-build && npm run landing-build && npm run live-build",
    "build:dev": "./node_modules/.bin/node-sass --watch ./src/static/css/scss/application.scss ./src/static/css/application.css",
    "react-build": "./node_modules/.bin/webpack --config webpack.config.js",
    "css-build": "./node_modules/.bin/node-sass ./src/static/css/scss/application.scss ./src/static/css/application.css",
    "landing-build": "./node_modules/.bin/node-sass ./src/static/css/scss/landingpage/landingpage.scss ./src/static/css/landingpage.css",
    "landing-build:dev": "./node_modules/.bin/node-sass --watch ./src/static/css/scss/landingpage/landingpage.scss ./src/static/css/landingpage.css",
    "live-build": "./node_modules/.bin/node-sass ./src/static/css/scss/livepage/livepage.scss ./src/static/css/livepage.css",
    "live-build:dev": "./node_modules/.bin/node-sass --watch ./src/static/css/scss/livepage/livepage.scss ./src/static/css/livepage.css",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannycho7/sbhacksiv.git"
  },
  "keywords": [],
  "author": "Danny Cho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dannycho7/sbhacksiv/issues"
  },
  "homepage": "https://github.com/dannycho7/sbhacksiv#readme",
  "dependencies": {
    "aws-sdk": "^2.82.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "bunyan": "^1.8.10",
    "express": "^4.15.2",
    "express-form-post": "^1.3.0",
    "express-session": "^1.15.2",
    "helmet": "^3.6.0",
    "morgan": "^1.8.1",
    "nodemailer": "^4.0.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "sequelize": "^4.2.1",
    "sequelize-cli": "^2.7.0",
    "serve-favicon": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "webpack": "^2.5.1"
  }
}
